   <define name="tei_zinep">
      <element name="p">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(paragraph) This rule align the p element with the div1 element [3.1. Paragraphs 7.2.5. Speech Contents]</a:documentation>
         <ref name="tei_zinemacro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
